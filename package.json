{
  "name": "mrmackey",
  "version": "1.0.0",
  "description": "Discord Bot",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint --cache --fix",
    "prettier": "prettier --write .",
    "prepare": "npx husky install",
    "check": "npm run lint && npm run prettier && npm run test",
    "start": "nohup ts-node -r tsconfig-paths/register index.ts > log.txt 2>&1 & echo $! > pid.txt",
    "stop": "kill $(tail pid.txt)",
    "deploy": "gcloud app deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CowDotDev/MrMackey.git"
  },
  "keywords": [
    "Discord",
    "Bot"
  ],
  "author": "CowDotDev",
  "license": "SEE LICENSE IN LICENSE",
  "bugs": {
    "url": "https://github.com/CowDotDev/MrMackey/issues"
  },
  "homepage": "https://github.com/CowDotDev/MrMackey#readme",
  "devDependencies": {
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-ban": "^1.6.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jest": "^25.2.4",
    "eslint-plugin-jsdoc": "^37.0.3",
    "eslint-plugin-modules-newline": "^0.0.6",
    "eslint-plugin-no-null": "^1.0.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "husky": "^7.0.4",
    "jest": "^27.3.1",
    "lint-staged": "^12.0.2",
    "prettier": "2.4.1",
    "ts-jest": "^27.0.7"
  },
  "lint-staged": {
    "*.{ts}": "eslint --cache --fix",
    "*.{js,json,ts,tsx,css,md}": "prettier --write"
  },
  "dependencies": {
    "@tsconfig/recommended": "^1.0.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@discordjs/builders": "^0.8.2",
    "@discordjs/rest": "^0.1.0-canary.0",
    "@google-cloud/secret-manager": "^3.10.1",
    "axios": "^0.24.0",
    "discord-api-types": "^0.24.0",
    "discord.js": "^13.3.1",
    "dotenv": "^10.0.0",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4",
    "tsconfig-paths": "^3.11.0",
    "tslib": "^2.3.1"
  }
}
